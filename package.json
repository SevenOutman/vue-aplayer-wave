{
  "name": "vue-aplayer-wave",
  "version": "1.0.0",
  "description": "Sound wave visualization plugin for vue-aplayer",
  "main": "src/vue-aplayer-wave.vue",
  "scripts": {
    "dev": "webpack-dev-server --inline --hot --progress --colors --host 0.0.0.0 --port 3000 --config webpack.demo.config.js",
    "build": "export NODE_ENV=production && webpack --progress --hide-modules",
    "build:demo": "export NODE_ENV=production && webpack --progress --hide-modules --config webpack.demo.config.js",
    "prepublish": "npm run build",
    "publish": "npm publish",
    "predeploy": "npm run build:demo",
    "deploy": "gh-pages -d demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SevenOutman/vue-aplayer-wave.git"
  },
  "keywords": [
    "aplayer",
    "vue-aplayer",
    "vue",
    "audio",
    "html5"
  ],
  "author": {
    "name": "Doma",
    "url": "https://github.com/SevenOutman",
    "email": "leishenghao@126.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SevenOutman/vue-aplayer-wave/issues"
  },
  "homepage": "https://github.com/SevenOutman/vue-aplayer-wave#readme",
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.26.0",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.24.1",
    "node-sass": "^4.5.0",
    "postcss-loader": "^2.0.9",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "vue": "^2.5.13",
    "vue-aplayer": "git+https://github.com/SevenOutman/vue-aplayer.git",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.6.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "vue": "^2.5.13"
  }
}
